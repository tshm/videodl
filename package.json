{
  "name": "videodl",
  "version": "0.0.2",
  "description": "videodl connector",
  "main": "index.mjs",
  "prettier": {
    "singleQuote": true
  },
  "dependencies": {
    "firebase": "^9.7.0",
    "pino": "^7.11.0",
    "pino-elasticsearch": "^6.2.0",
    "sanitize-filename": "^1.6.3",
    "shelljs": "^0.8.5"
  },
  "devDependencies": {
    "@types/shelljs": "^0.8.11",
    "pino-dev": "^2.0.13",
    "prettier": "^2.6.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tshm/videodl.git"
  },
  "scripts": {
    "run": "node index.mjs . | pino-dev",
    "test": "env DRY_RUN=true node index.mjs . | pino-dev",
    "docker": "docker build -t videodl -t videodl:$(jq -r .version package.json)-$(git rev-parse --short HEAD) .",
    "build": "act build",
    "start": "act start",
    "watch": "act start -w",
    "act": "act --workflows ./dist/workflows --secret-file ./dist/.secrets --eventpath ./dist/event.json --env-file ./dist/.env -P ubuntu-latest=actionsflow/act-environment:v1 -P ubuntu-18.04=actionsflow/act-environment:v1",
    "clean": "act clean"
  },
  "author": "",
  "license": "ISC"
}
